<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Suite1">
  <groups>
    <run>
      <include name="smoke"></include>
    </run>
  </groups>

  <listeners>
    <listener class-name="Listener.ListenerEx"></listener>
  </listeners>

  <test name="Authen" >
    <classes>
      <class name="Authen.NormalLogin"/>
      <class name="Authen.SSOLogin"/>
    </classes>
  </test>

  <test name="Task" >
    <classes>
      <class name="Task.TestCRUD"/>
      <class name="Task.TestManagement"/>
    </classes>
  </test>
</suite>
